{
	"name": "too-many-tabs",
	"version": "1.0.3",
	"description": "",
	"main": "popup.js",
	"scripts": {
		"icons": "node ./bin/icon.js",
		"dev:ff": "web-ext run -v -s ./src",
		"build:ff": "web-ext build --overwrite-dest -s ./src",
		"clean:ff": "rm web-ext-artifacts/too_many_tabs-*.zip"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@eslint/js": "9.9.0",
		"cz-conventional-changelog": "3.2.0",
		"eslint": "9.9.0",
		"globals": "15.9.0",
		"web-ext": "8.2.0",
		"webextension-polyfill": "0.12.0"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"prettier": {
		"tabWidth": 2,
		"semi": true,
		"singleQuote": false,
		"useTabs": true,
		"printWidth": 80,
		"overrides": [
			{
				"files": [
					"*.yml",
					"*.yaml"
				],
				"options": {
					"useTabs": false
				}
			}
		]
	}
